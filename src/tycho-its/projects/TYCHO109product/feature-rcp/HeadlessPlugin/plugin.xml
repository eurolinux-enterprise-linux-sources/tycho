<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="headlessplugin.Application">
         </run>
      </application>
   </extension>
   <extension
         id="HeadlessPlugin"
         point="org.eclipse.core.runtime.products">
      <product
            application="HeadlessPlugin.application"
            name="Headless Feature">
         <property
               name="appName"
               value="Headless Feature">
         </property>
      </product>
   </extension>

</plugin>
